---
import BaseLayout from '../layouts/BaseLayout.astro'

import PageHeader from '../components/PageHeader.astro'
import PageIntro from '../components/PageIntro.astro'
import Form from '../components/Form.astro'
---

<BaseLayout title="Book Your Appointment Today">
    <PageHeader title="Book Your Appointment" image="contact" />
    <main>
        <PageIntro introTitle="Visit us soon" text="Use the form below to contact us. We will get back to you as soon as we can!" />
        <div class="">
            <div class="px-4 mx-auto mb-8 sm:mb-16 max-w-7xl sm:px-6 lg:px-8">
                <div class="max-w-lg mx-auto text-center md:max-w-none">
                <div>
                    <h2 class="text-2xl font-extrabold text-gray-900 sm:text-3xl">Contact Information</h2>
                    <div class="flex flex-col items-center justify-center gap-6 mt-8 sm:flex-row ">
                        <div class="flex">
                            <div class="flex-shrink-0">
                            <!-- Heroicon name: outline/phone -->
                            <svg class="w-6 h-6 text-gray-500" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" aria-hidden="true">
                                <path stroke-linecap="round" stroke-linejoin="round" d="M3 5a2 2 0 012-2h3.28a1 1 0 01.948.684l1.498 4.493a1 1 0 01-.502 1.21l-2.257 1.13a11.042 11.042 0 005.516 5.516l1.13-2.257a1 1 0 011.21-.502l4.493 1.498a1 1 0 01.684.949V19a2 2 0 01-2 2h-1C9.716 21 3 14.284 3 6V5z" />
                            </svg>
                            </div>
                            <div class="ml-3 text-base text-gray-600">
                            <p>201-630-0770</p>
                            </div>
                        </div>
                        <div class="flex">
                            <div class="flex-shrink-0">
                            <!-- Heroicon name: outline/mail -->
                            <svg class="w-6 h-6 text-gray-500" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" aria-hidden="true">
                                <path stroke-linecap="round" stroke-linejoin="round" d="M3 8l7.89 5.26a2 2 0 002.22 0L21 8M5 19h14a2 2 0 002-2V7a2 2 0 00-2-2H5a2 2 0 00-2 2v10a2 2 0 002 2z" />
                            </svg>
                            </div>
                            <div class="ml-3 text-base text-gray-600">
                            <p>bodywork@quantumvita.com</p>
                            </div>
                        </div>
                    </div>
                </div>
                </div>
            </div>
        </div>
        <Form />
        <section class="mt-8 sm:mt-12">
            <PageIntro introTitle="What clients are saying" text="" />
            <div class="grid gap-8 sm:grid-cols-2 content-wrapper">
                <span class="yelp-review" data-review-id="A_-_p0fm5XB63biTofkV7g" data-hostname="www.yelp.com">Read <a href="https://www.yelp.com/user_details?userid=gb5mzSEbTW0lnyOxlsW5iQ" rel="nofollow noopener">Tim A.</a>'s <a href="https://www.yelp.com/biz/quantum-vita-body-therapy-jersey-city?hrid=A_-_p0fm5XB63biTofkV7g" rel="nofollow noopener">review</a> of <a href="https://www.yelp.com/biz/lUVhkADHCiIANJ5R8KIkjQ" rel="nofollow noopener">Quantum Vita Body Therapy</a> on <a href="https://www.yelp.com" rel="nofollow noopener">Yelp</a></span>
                <span class="yelp-review" data-review-id="pYKVV3A6czvwJ9c7gJMmWQ" data-hostname="www.yelp.com">Read <a href="https://www.yelp.com/user_details?userid=3B53EcNJ6axLvEN5w_yhuA" rel="nofollow noopener">Danielle B.</a>'s <a href="https://www.yelp.com/biz/quantum-vita-body-therapy-jersey-city?hrid=pYKVV3A6czvwJ9c7gJMmWQ" rel="nofollow noopener">review</a> of <a href="https://www.yelp.com/biz/lUVhkADHCiIANJ5R8KIkjQ" rel="nofollow noopener">Quantum Vita Body Therapy</a> on <a href="https://www.yelp.com" rel="nofollow noopener">Yelp</a></span>
                <span class="yelp-review" data-review-id="dkIOeulH6fC4nZvF3FzgLA" data-hostname="www.yelp.com">Read <a href="https://www.yelp.com/user_details?userid=-Knno0IOe_6GgFalRjADFg" rel="nofollow noopener">Alissa M.</a>'s <a href="https://www.yelp.com/biz/quantum-vita-body-therapy-jersey-city?hrid=dkIOeulH6fC4nZvF3FzgLA" rel="nofollow noopener">review</a> of <a href="https://www.yelp.com/biz/lUVhkADHCiIANJ5R8KIkjQ" rel="nofollow noopener">Quantum Vita Body Therapy</a> on <a href="https://www.yelp.com" rel="nofollow noopener">Yelp</a></span>
                <span class="yelp-review" data-review-id="UvNwsSdvtPMgB4ZMSuBkkw" data-hostname="www.yelp.com">Read <a href="https://www.yelp.com/user_details?userid=EocEi5etzhS6NZOPT-EJRQ" rel="nofollow noopener">Elizabeth L.</a>'s <a href="https://www.yelp.com/biz/quantum-vita-body-therapy-jersey-city?hrid=UvNwsSdvtPMgB4ZMSuBkkw" rel="nofollow noopener">review</a> of <a href="https://www.yelp.com/biz/lUVhkADHCiIANJ5R8KIkjQ" rel="nofollow noopener">Quantum Vita Body Therapy</a> on <a href="https://www.yelp.com" rel="nofollow noopener">Yelp</a></span>
            </div>
        </section>
    </main>
    <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3023.754420170714!2d-74.05278018419787!3d40.723422379330536!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x89c250b21f8d900d%3A0x2178a6c5708beb42!2sQuantum%20Vita%20Body%20Therapy!5e0!3m2!1sen!2sus!4v1629159100639!5m2!1sen!2sus" width="100%" height="450" style="border:0;" allowfullscreen="" loading="lazy" class="mt-24"></iframe>
</BaseLayout>

<script src="https://www.yelp.com/embed/widgets.js" type="text/javascript" async></script>
